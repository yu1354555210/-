
/**
     * 历史记录
 */
.entry-main {
  padding: 23rpx 14rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.entry-main .entry-shortcut-nav {
  background-color: #fff;
  height: 94rpx;
  line-height: 94rpx;
  overflow: hidden;
}
.entry-main .entry-shortcut-nav ul {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.entry-main .entry-shortcut-nav ul li {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  text-align: center;
  position: relative;
}
.entry-main .entry-shortcut-nav ul li:after {
  height: 100%;
  content: '';
  width: 1px;
  border-right: 1px solid #b2b2b2;
  position: absolute;
  top: 0;
  right: 0;
  transform: scaleX(0.5);
  -webkit-transform: scaleX(0.5);
  z-index: 10;
}
.entry-main .entry-shortcut-nav ul li:last-child:after {
  border-right: none;
}
.entry-main .entry-shortcut-nav ul li navigator {
  height: 100%;
  color: #333333;
  display: block;
  font-size: 30rpx;
}
.entry-main .entry-shortcut-nav ul li navigator text {
  display: inline-block;
  vertical-align: middle;
}
.entry-main .entry-shortcut-nav ul li navigator .entry-shortcut-icon {
  width: 50rpx;
  height: 50rpx;
  background-size: 100% 100%;
}
.entry-main .entry-shortcut-nav ul li navigator text.icon-board {
  background-image: url(https://api-yxdl.binglue.com/img/short-icon01.png);
  margin-right: 10rpx;
}
.entry-main .entry-shortcut-nav ul li navigator text.icon-star {
  background-image: url(https://api-yxdl.binglue.com/img/short-icon02.png);
}
.entry-main .entry-shortcut-nav ul li navigator text.icon-time {
  background-image: url(https://api-yxdl.binglue.com/img/short-icon03.png);
}
.entry-main .entry-shortcut-nav ul li navigator:active {
  background-color: rgba(233, 233, 233, 0.5);
}
/**
     * 历史记录飞行员卡片
 */
