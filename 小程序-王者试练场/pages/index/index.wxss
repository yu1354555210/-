}
.flyer-card-box .flyer-card-item .flyer-card-footer:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  color: #d9d9d9;
  border-top: 1px solid #dbdbdb;
  -webkit-transform-origin: top left;
  transform-origin: top left;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
