.fly-content {
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
.fly-top-image {
  width: 100%;
  height: 258rpx;
  overflow: hidden;
}
.fly-go-item {
  background-color: #fff;
  margin-bottom: 30rpx;
  overflow: hidden;
  position: relative;
}
.fly-go-item:after {
  content: " ";
  position: absolute;
  left: 0;
  width: 100%;
  height: 1px;
  color: #d9d9d9;
  bottom: 0;
  border-bottom: 1px solid #dbdbdb;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.fly-go-item .fly-go-title {
  height: 92rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
  font-size: 30rpx;
  color: #333333;
  padding: 0 23rpx;
  box-sizing: border-box;
  position: relative;
}
.fly-go-item .fly-go-title:after {
  content: " ";
  position: absolute;
  left: 0;
  width: 100%;
  height: 1px;
  color: #d9d9d9;
  bottom: 0;
  border-bottom: 1px solid #dbdbdb;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.fly-go-item .fly-go-title span {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
}
.fly-go-item .fly-go-title span i {
  margin-right: 14rpx;
  font-size: 38rpx;
}
.fly-go-item .fly-go-title text,.fly-go-item .fly-go-title .picker {
  color: #ff8a00;
  font-size: 26rpx;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
}

.fly-go-item .fly-go-title span .icon-right {
  color: #333;
  font-size: 21rpx;
  margin: 0 28rpx 0 5rpx;
}
.fly-go-item .fly-go-title .picker i{
	color: #333;
	font-size: 21rpx;
	margin: 0 28rpx 0 5rpx;
}
.fly-go-item .fly-go-main {
  overflow: hidden;
}
.fly-go-item .fly-go-main .this-big-btn.active {
  background-color: #0b91ff;
  color: #fff;
}
.fly-go-item .fly-go-main .this-big-btn.this-empty-btn {
  background-color: transparent;
  opacity: 0;
}
.fly-go-item .fly-go-main .fly-go-hint {
  font-size: 20rpx;
  padding: 27rpx 0 28rpx 24rpx;
  box-sizing: border-box;
  color: #ff0000;
}
.fly-go-item .fly-go-main .fly-go-hint span {
  margin-left: 10rpx;
  color: #666666;
}
.fly-go-item .fly-go-main .fly-go-form {
  padding: 38rpx 58rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  flex-wrap: wrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
  -webkit-flex-wrap: wrap;
  box-sizing: border-box;
}
.fly-go-item .fly-go-main .fly-go-form .this-big-btn {
  margin-bottom: 38rpx;
}
.fly-go-item .fly-go-main .fly-go-form .fly-go-inputbox {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  flex-direction: column;
}
.fly-go-item .fly-go-main .fly-go-form .fly-go-inputbox li {
  width: 100%;
  height: 90rpx;
  margin-bottom: 44rpx;
  position: relative;
  display: block;
}
.fly-go-item .fly-go-main .fly-go-form .fly-go-inputbox li i {
  color: #8e8e8e;
  position: absolute;
  left: 24rpx;
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  font-size: 36rpx;
}
.fly-go-item .fly-go-main .fly-go-form .fly-go-inputbox li input {
  width: 100%;
  padding-left: 74rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0;
  background-color: #e9e9e9;
  color: #333;
  font-size: 30rpx;
  border: none;
  border-radius: 5rpx;
  box-sizing: border-box;
}
.fly-go-item .fly-go-main .fly-go-form .fly-go-inputbox li input::-webkit-input-placeholder {
  color: #c1c1c1;
}

/**
     * 优惠券弹出层
 */
.coupons-winpop-container {
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  position: absolute;
  z-index: 888;
  background-color: #efeff4;
}
.coupons-winpop-container .coupons-main {
  padding: 50rpx 0;
  box-sizing: border-box;
}
.coupons-item {
  padding: 68rpx 5rpx;
  box-sizing: border-box;
  height: 287rpx;
  width: 100%;
  background: url(https://api-yxdl.binglue.com/img/coupons-bg.png) no-repeat;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
  margin-bottom: -20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
}
.coupons-item:last-child {
  margin-bottom: 200rpx;
}
.coupons-item.coupons-item-disabled .coupons-fl {
  color: #999;
}
.coupons-item.coupons-item-disabled .coupons-fl dt span,
.coupons-item.coupons-item-disabled .coupons-fl dd,
.coupons-item.coupons-item-disabled .coupons-fl dd span {
  color: #999;
}
.coupons-item.coupons-item-disabled .coupons-fr .coupons-btn {
  background: #999;
}
.coupons-item.coupons-item-disabled .coupons-fr dd {
  color: #999;
  display: block;
}
.coupons-item .coupons-fl {
  flex: 0 0 52%;
  -webkit-flex: 0 0 52%;
  text-align: center;
  color: #ff9c00;
}
.coupons-item .coupons-fl dt {
  font-size: 24rpx;
  margin-bottom: 10rpx;
  line-height: 40rpx;
  display: block;
}
.coupons-item .coupons-fl dt strong {
  font-size: 108rpx;
  margin-right: 5rpx;
}
.coupons-item .coupons-fl dt span {
  color: #333;
  margin-left: 5rpx;
}
.coupons-item .coupons-fl dd {
  font-size: 22rpx;
  color: #666666;
  display: block;
}
.coupons-item .coupons-fl dd span {
  margin-right: 5rpx;
  color: #333333;
}
.coupons-item .coupons-fr {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
}
.coupons-item .coupons-fr dl {
  /* padding-left: 25rpx; */
  line-height: 40rpx;
  box-sizing: border-box;
}
.coupons-item .coupons-fr dt {
  color: #999;
}
.coupons-item .coupons-fr dt p {
  font-size: 20rpx;
}
.coupons-item .coupons-fr dd .coupons-btn {
  width: 165rpx;
  height: 57rpx;
  line-height: 57rpx;
  color: #fff;
  background-color: #ff2424;
}
.coupons-item .coupons-fr dd .coupons-btn.coupons-btn-end {
  background-color: #999;
}